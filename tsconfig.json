{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "CommonJS",
    "strict": false,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ],
  "exclude": [
    "**/node_modules"
  ]
}
